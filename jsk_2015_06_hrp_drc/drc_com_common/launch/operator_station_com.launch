<launch>
  <node pkg="jsk_network_tools" type="silverhammer_highspeed_receiver.py"
        output="screen"
        name="highspeed_receiver">
    <rosparam>
      message: drc_com_common/FC2OCSLarge
      topic_prefix: "/fc"
      receive_ip: "0.0.0.0"
    </rosparam>
  </node>
  <!-- relay -->
  <include file="$(find drc_com_common)/launch/ocs_relay.launch" if="false"/>
</launch>
