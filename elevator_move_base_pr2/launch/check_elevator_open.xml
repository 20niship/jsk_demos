<launch>
  <arg name="input" default="/openni_c2/depth_registered_half/points" />
  <arg name="output" default="/elevator_inside_change_cloud" />

  <node name="elevator_inside_change_detector" pkg="jsk_pcl_ros" type="octree_change_publisher">
    <param name="resolution" value="0.07" />
    <param name="noise_filter" value="0.2" />
    <remap from="~input" to="$(arg input)" />
    <remap from="~octree_change_result" to="$(arg output)" />
  </node>

</launch>